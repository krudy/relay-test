{
  "name": "react-relay-experiments",
  "description": "A simple react app connected to an in-memory GraphQL server.",
  "version": "0.0.15",
  "dependencies": {
    "graphql": "0.11.7",
    "graphql-tools": "2.5.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-relay": "^1.4.1"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.4.1",
    "react-scripts": "1.0.0",
    "relay-compiler": "^1.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema src/schema.graphql"
  }
}
